@page
@using Genesis.Attendance.Localization
@using Genesis.Attendance.Web.Pages.Attendance.Teacher
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<AttendanceResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="modelDto" asp-page="/Attendance/Teacher/CreateModal">
    <abp-modal>
        <abp-modal-header title="Create @L["Menu:Teacher"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
